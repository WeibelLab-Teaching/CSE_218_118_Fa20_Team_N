{
  "compilerOptions": {
    "outDir": "./lib",
    "target": "es5",
    "lib": ["es6", "dom"],
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "types": [ "babylonjs", "babylonjs-loaders", "node", "jquery" ]
  },
  "allowSyntheticDefaultImports": true,
  "include": [
    "src/**/*.ts"
  ]
}
