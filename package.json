{
  "name": "babylonjs-colyseus",
  "version": "1.0.0",
  "description": "Boilerplate intended to be used for the BabylonJS 2.5 Challenge",
  "main": "server/lib/index.js",
  "scripts": {
    "compile-server": "npm install --prefix server && tsc -p server",
    "compile-client": "npm install --prefix client && cd client && ./node_modules/.bin/webpack --env.production && cd -",
    "install": "npm run compile-server && npm run compile-client",
    "start": "node server/lib/index.js"
  },
  "engines": {
    "node": "8.7.0"
  },
  "repository": {
    "url": "git@github.com:endel/babylonjs-multiplayer-boilerplate.git",
    "type": "git"
  },
  "author": "Endel Dreyer <endel.dreyer@gmail.com>",
  "license": "Apache 2.0",
  "devDependencies": {
    "@types/get-image-colors": "^2.0.0",
    "@types/pngjs": "^3.4.2",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "@types/express": "^4.17.9",
    "@types/jquery": "^3.5.4",
    "@types/socket.io": "^2.1.11",
    "babylonjs": "^4.2.0",
    "base64-arraybuffer": "^0.2.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "fs": "^0.0.1-security",
    "get-image-colors": "^4.0.0",
    "get-pixels": "^3.3.2",
    "http": "^0.0.1-security",
    "node-fetch": "^2.6.1",
    "pngjs": "^3.4.0",
    "socket.io": "^3.0.3"
  }
}
